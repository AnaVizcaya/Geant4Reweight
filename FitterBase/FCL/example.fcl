#include "sets.fcl"
#include "parameters.fcl"
#include "C_experiments.fcl"

OutputFile: "example.root"
FitScan: false
Save: false

#Taken from sets.fcl
Sets: [
  @local::C_PiPlus
]

#Taken from C_experiments.fcl
Experiments: [ 
  @sequence::C_experiments
]

#Taken from parameters.fcl
ParameterSet: @local::TheParameters

IncludeDUET: true
DUETDataFile: "/path/to/DUET.root"

MaxCalls: 500
Tolerance: 1.e-5
UpperLimit: 2.0 
LowerLimit: .5
